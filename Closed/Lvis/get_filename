import os
import codecs

rs = codecs.open("get_filename_rs.txt", "+w", encoding="utf-8")
url = r"\\server10\Sotatek\01102019_Request\Raw\4"

for dirs, folders, files in os.walk(url):
    for file in files:
        if file.endswith("json"):
            continue
        if file.endswith("db"):
            continue
        # rs.write(dirs + "\\" + file + "\n")
        rs.write(dirs + "+" + file + "\n")